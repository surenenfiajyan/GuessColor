{
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
	},
	"minimum_chrome_version": "93",
	"name": "Guess Color",
	"description": "Guess the colors with as few attempts as possible",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "js/background-script.js",
		"type": "module"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {},
	"permissions": [
		"storage",
		"downloads",
		"tabs"
	]
}